@using Dota2.Extensions
<!DOCTYPE html>

<html>
<head>
    <!--Import Google Icon Font-->
    <link href="http://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">

    <!--Import materialize.css-->
    <link href="~/Content/css/libs/materialize/materialize.min.css" type="text/css" media="screen,projection" rel="stylesheet" />
    <!--CORE CSS-->
    <link href="~/Content/css/libs/materialize/style.min.css" type="text/css" media="screen,projection" rel="stylesheet" />
    <!--CUSTOM CSS TO OVVERIDE CURRENT-->
    <link href="~/Content/css/libs/materialize/custom.css" type="text/css" media="screen,projection" rel="stylesheet" />

    <!-- INCLUDED CSS PLUGIN ON THIS PAGE -->
    @RenderSection("styles", required: false)
    <link href="~/Scripts/libs/materialize/plugins/perfect-scrollbar/perfect-scrollbar.css" rel="stylesheet" media="screen,projection"/>
    <link href="~/Scripts/libs/materialize/plugins/sweetalert/sweetalert.css" rel="stylesheet"/>

    <link href="~/Scripts/libs/materialize/plugins/jvectormap/jquery-jvectormap.css" type="text/css" rel="stylesheet" media="screnn,projection" />
    <!--Let browser know website is optimized for mobile-->
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />


    <!-- Favicons-->
    <!--TODO Need to set correct favicons-->
    <link rel="icon" href="/d2.ico" sizes="32x32">
    @*
        <!-- Favicons-->
        <link rel="apple-touch-icon-precomposed" href="images/favicon/apple-touch-icon-152x152.png">
        <!-- For iPhone -->
        <meta name="msapplication-TileColor" content="#00bcd4">
        <meta name="msapplication-TileImage" content="images/favicon/mstile-144x144.png">*@
    <!-- For Windows Phone -->
    <title>Dota2 api</title>
</head>
<body>

    <!-- Start Page Loading -->
    <div id="loader-wrapper">
        <div id="loader"></div>
        <div class="loader-section section-left"></div>
        <div class="loader-section section-right"></div>
    </div>
    <!-- End Page Loading -->
    <!-- START HEADER -->
    <header id="header" class="page-topbar">
        <!-- start header nav-->
        <div class="navbar-fixed">
            <nav class="navbar-color">
                <div class="nav-wrapper">
                    <ul class="left">
                        <li>
                            <h1 class="logo-wrapper">
                                <a href="#" class="brand-logo darken-1">
                                    @*<img src="@Url.Content("~/Content/img/logo.png")" alt="materialize logo">*@
                                </a>
                                <span class="logo-text">Materialize</span>
                            </h1>
                        </li>
                    </ul>
                    <div class="header-search-wrapper hide-on-med-and-down">
                        <i class="mdi-action-search"></i>
                        <input type="text" name="Search" class="header-search-input z-depth-2" placeholder="Explore Materialize" />
                    </div>
                    <ul class="right hide-on-med-and-down">
                        @*<li>
                                <a href="javascript:void(0);" class="waves-effect waves-block waves-light translation-button" data-activates="translation-dropdown"><img src="images/flag-icons/United-States.png" alt="USA" /></a>
                            </li>*@
                        <li>
                            <a href="javascript:void(0);" class="waves-effect waves-block waves-light toggle-fullscreen"><i class="mdi-action-settings-overscan"></i></a>
                        </li>
                        <li>
                            <a href="javascript:void(0);" class="waves-effect waves-block waves-light notification-button" data-activates="notifications-dropdown">
                                <i class="mdi-social-notifications"><small class="notification-badge">5</small></i>

                            </a>
                        </li>
                        <li>
                            <a href="#" id="activate-chat" data-activates="chat-out" class="waves-effect waves-block waves-light chat-collapse"><i class="mdi-communication-chat"></i></a>
                        </li>
                    </ul>
                    <!-- translation-button -->
                    @*<ul id="translation-dropdown" class="dropdown-content">
                            <li>
                                <a href="#!"><img src="images/flag-icons/United-States.png" alt="English" />  <span class="language-select">English</span></a>
                            </li>
                            <li>
                                <a href="#!"><img src="images/flag-icons/France.png" alt="French" />  <span class="language-select">French</span></a>
                            </li>
                            <li>
                                <a href="#!"><img src="images/flag-icons/China.png" alt="Chinese" />  <span class="language-select">Chinese</span></a>
                            </li>
                            <li>
                                <a href="#!"><img src="images/flag-icons/Germany.png" alt="German" />  <span class="language-select">German</span></a>
                            </li>

                        </ul>*@
                    <!-- notifications-dropdown -->
                    <ul id="notifications-dropdown" class="dropdown-content">
                        <li>
                            <h5>NOTIFICATIONS <span class="new badge">5</span></h5>
                        </li>
                        <li class="divider"></li>
                        <li>
                            <a href="#!"><i class="mdi-action-add-shopping-cart"></i> A new order has been placed!</a>
                            <time class="media-meta" datetime="2015-06-12T20:50:48+08:00">2 hours ago</time>
                        </li>
                        <li>
                            <a href="#!"><i class="mdi-action-stars"></i> Completed the task</a>
                            <time class="media-meta" datetime="2015-06-12T20:50:48+08:00">3 days ago</time>
                        </li>
                        <li>
                            <a href="#!"><i class="mdi-action-settings"></i> Settings updated</a>
                            <time class="media-meta" datetime="2015-06-12T20:50:48+08:00">4 days ago</time>
                        </li>
                        <li>
                            <a href="#!"><i class="mdi-editor-insert-invitation"></i> Director meeting started</a>
                            <time class="media-meta" datetime="2015-06-12T20:50:48+08:00">6 days ago</time>
                        </li>
                        <li>
                            <a href="#!"><i class="mdi-action-trending-up"></i> Generate monthly report</a>
                            <time class="media-meta" datetime="2015-06-12T20:50:48+08:00">1 week ago</time>
                        </li>
                    </ul>
                </div>
            </nav>
        </div>
        <!-- end header nav-->
    </header>
    <!-- END HEADER -->
    <!-- START MAIN -->
    <div id="main">
        <!-- START WRAPPER -->
        <div class="wrapper">
            <!--LEFT SIDEBA NAV-->
            <aside id="left-sidebar-nav">
                <ul id="slide-out" class="side-nav fixed leftside-navigation ps-container ps-active-y" style="width: 240px; height: 100%">
                    <li class="user-details cyan darken-2">
                        <div class="row">
                            <div class="col col s4 m4 14">
                                <img src="@Url.Content("~/Content/img/user.jpg")" alt="" class="circle responsive-img valign profile-image" />
                            </div>
                            <div class="col col s8 m8 18">
                                <a class="btn-flat dropdown-button waves-effect waves-light white-text profile-btn" href="#" data-activates="profile-dropdown">
                                    User name
                                    <i class="mdi-navigation-arrow-drop-down right"></i>
                                </a>
                                <ul id="profile-dropdown" class="dropdown-content">
                                    <li>
                                        <a href="#">
                                            <i class="mdi-action-face-unlock"></i>Profile
                                        </a>
                                    </li>
                                    <li>
                                        <a href="#">
                                            <i class="mdi-action-settings"></i> Settings
                                        </a>
                                    </li>
                                    <li>
                                        <a href="#">
                                            <i class="mdi-communication-live-help"></i> Help
                                        </a>
                                    </li>
                                    <li class="divider"></li>
                                    <li>
                                        <a href="#">
                                            <i class="mdi-action-lock-outline"></i> Lock
                                        </a>
                                    </li>
                                    <li>
                                        <a href="#">
                                            <i class="mdi-hardware-keyboard-tab"></i> Logout
                                        </a>
                                    </li>
                                </ul>
                                <p class="user-roal">Uer role</p>
                            </div>
                        </div>
                    </li>

                    <li class="bold @Html.IsActive(actions: "Index", controllers: "Heroes")">
                        <a href="@Url.Action("GetHeroesPartial", "Heroes")" data-get="heroes" class="waves-effec waves-cyan">
                            <i class="mdi-action-account-circle"></i>
                            Heroes
                        </a>
                    </li>
                    <li class="bold @Html.IsActive(actions: "Index", controllers: "Stats")">
                        <a href="@Url.Action("GetStatsPartial", "Stats")" data-get="stats" class="waves-effect waves-ripple">
                            <i class="mdi-device-now-widgets"></i>
                            Game statistic
                        </a>
                    </li>
                    <li>
                        <a class="waves-effect waves-ripple">Call web API</a>
                    </li>

                </ul>
                <a href="#" data-activates="slide-out" class="sidebar-collapse btn-floating btn-medium waves-effect waves-light hide-on-large-only cyan">
                    <i class="mdi-navigation-menu"></i>12
                </a>
            </aside>
            <!-- END LEFT SIDEBAR NAV-->
            <!-- START CONTENT -->
            <section id="content">
                <div class="container" id="container">
                    @RenderBody()
                </div>

            </section>
            <!-- END CONTENT -->
            <!-- START RIGHT SIDEBAR NAV-->
            <aside id="right-sidebar-nav">
                <ul id="chat-out" class="side-nav rightside-navigation right-aligned ps-container ps-active-y" style="width: 300px; right: 0px; height: 429px">
                    <li class="li-hover">
                        <a href="#" data-activates="chat-out" class="chat-close-collapse right">
                            <i class="mdi-navigation-close"></i>
                        </a>
                        <div id="right-search" class="row">
                            <form class="col s12">
                                <div class="input-field">
                                    <i class="mdi-action-search prefix"></i>
                                    <input id="icon_prefix" type="text" class="validate">
                                    <label for="icon_prefix">Search</label>
                                </div>
                            </form>
                        </div>
                    </li>
                    <li class="li-hover">
                        <ul class="chat-collapsible" data-collapsible="expandable">
                            <li class="active">
                                <div class="collapsible-header teal white-text active">
                                    <i class="mdi-social-whatshot"></i>Recent Activity
                                </div>
                                <div class="collapsible-body recent-activity" style="display: block">
                                    <div class="recent-activity-list chat-out-list row">
                                        <div class="col s3 recent-activity-list-icon">
                                            <i class="mdi-action-add-shopping-cart"></i>
                                        </div>
                                        <div class="col s9 recent-activity-list-text">
                                            <a href="#">just now</a>
                                            <p>Jim Doe Purchased new equipments for zonal office.</p>
                                        </div>
                                    </div>
                                </div>
                            </li>
                        </ul>
                    </li>
                </ul>
            </aside>
            <!-- END RIGHT SIDEBAR NAV-->
        </div>
        <!-- END WRAPPER -->
    </div>
    <!-- END MAIN -->
    <!-- //////////////////////////////////////////////////////////////////////////// -->
    <!-- START FOOTER -->
    <footer class="page-footer"></footer>
    <!-- END FOOTER -->
    <!-- ================================================
        Scripts
        ================================================ -->
    <!--Jquery Library -->
    @Scripts.Render("~/bundles/jquery")
    <!--Unobtrusive ajax-->
    <script src="~/Scripts/jquery.validate.min.js"></script>
    <script src="~/Scripts/jquery.unobtrusive-ajax.min.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>

    <!--floatThead-->
    @Scripts.Render("~/Scripts/libs/materialize/plugins/floatThead/jquery.floatThead.min.js")
    @Scripts.Render("~/Scripts/libs/materialize/plugins/floatThead/jquery.floatThead-slim.min.js")
    <!--knockout & knockout mapping-->
    @Scripts.Render("~/Scripts/libs/knockout/knockout.min.js")
    @Scripts.Render("~/Scripts/libs/knockout/knockout.mapping.min.js")
    <!--materialize js-->
    @Scripts.Render("~/Scripts/libs/materialize/materialize.min.js")
    <!--scrollbar-->
    @Scripts.Render("~/Scripts/libs/materialize/plugins/perfect-scrollbar/perfect-scrollbar.min.js")
    <!--chart js-->
    @Scripts.Render("~/Scripts/libs/materialize/plugins/chartjs/chart.min.js")
    @Scripts.Render("~/Scripts/libs/materialize/plugins/chartjs/chart-script.js")

    <!--sparkline-->
    @Scripts.Render("~/Scripts/libs/materialize/plugins/sparkline/jquery.sparkline.min.js")
    @Scripts.Render("~/Scripts/libs/materialize/plugins/sparkline/sparkline-script.js")

    <!--google map api-->
    @Scripts.Render("https://maps.googleapis.com/maps/api/js?key=AIzaSyAAZnaZBXLqNBRXjd-82km_NO7GUItyKek")

    <!--jvectormap-->
    @Scripts.Render("~/Scripts/libs/materialize/plugins/jvectormap/jquery-jvectormap-1.2.2.min.js")
    <!--display script for map-->
    @Scripts.Render("~/Scripts/libs/materialize/plugins/jvectormap/jquery-jvectormap-world-mill-en.js")
    @Scripts.Render("~/Scripts/libs/materialize/plugins/jvectormap/vectormap-script.js")

    <!--google map-->
    @Scripts.Render("~/Scripts/libs/materialize/plugins/google-map/google-map-script.js")

    <!--plugins.js - Some Specific JS codes for Plugin Settings-->
    @Scripts.Render("~/Scripts/libs/materialize/plugins/plugins.js")

    <!--Sweet alert-->
    @Scripts.Render("~/Scripts/libs/materialize/plugins/sweetalert/sweetalert.min.js")

    <!--custom-script.js - Add your own theme custom JS-->
    @Scripts.Render("~/Scripts/common.js")
    @Scripts.Render("~/Scripts/knockout-extensions.js")
    <script src="~/Scripts/menu.js"></script>
    @RenderSection("scripts", false)

    <div id="loader-container-wrapper" style="display: none">
        <div id="container-loader"></div>
        <div class="loader-section section-left"></div>
        <div class="loader-section section-right"></div>
    </div>
</body>
</html>
